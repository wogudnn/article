<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
<title>table</title>
</head>
<body>
	<table border="1" style="width: 300px">
		<tr>
			<td colspan="6" style="height: 20px" ></td>
		</tr>
		<tr>
			<td>7</td>
			<td>8</td>
			<td>9</td>
			<td>+</td>
			<td>%</td>
			<td>Clear</td>
		</tr>
		<tr>
			<td>4</td>
			<td>5</td>
			<td>6</td>
			<td>-</td>
			<td>(</td>
			<td>←</td>
		</tr>
		<tr>
			<td>1</td>
			<td>2</td>
			<td>3</td>
			<td>×</td>
			<td>)</td>
			<td rowspan="2">=</td>
		</tr>
		<tr>
			<td>0</td>
			<td>00</td>
			<td>.</td>
			<td>/</td>
			<td>+/-</td>
		</tr>
	</table>
	<table>
	
	</table>
</body>
</html>